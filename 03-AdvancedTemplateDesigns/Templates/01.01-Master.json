{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.1",
    "parameters": {
        "serviceLevel": {
            "type": "string",
            "allowedValues": [
                "Gold",
                "Diamond"
            ]
        },
        "vnetName": {
            "type": "string"
        },
        "publicIpName": {
            "type": "string"
        },
        "nicName": {
            "type": "string"
        },
        "vmName": {
            "type": "string"
        },
        "vmOsSku": {
            "type": "string",
            "allowedValues": [
                "Windows-Server-Technical-Preview"
            ]
        },
        "dnsPrefix": {
            "type": "string"
        },
        "adminUsername": {
            "type": "string"
        },
        "adminPassword": {
            "type": "securestring"
        }
    },
    "variables": {
        "location": "[resourceGroup().location]",
        "templateUriBase": "https://raw.githubusercontent.com/peterschen/ARM/master/03-AdvancedTemplateDesigns/Templates",
        "storageName": "[uniqueString(resourceGroup().id)]"
    },
    "resources": [
        {
            "apiVersion": "2015-11-01",
            "type": "Microsoft.Resources/deployments",
            "name": "[concat('VM-', parameters('serviceLevel'))]",
            "properties": {
                "mode": "[deployment().properties.mode]",
                "templateLink": {
                    "uri": "[concat(variables('templateUriBase'), '/01.02-VM-', parameters('serviceLevel'), '.json')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[variables('location')]"
                    },
                    "templateUriBase": {
                        "value": "[variables('templateUriBase')]"
                    },
                    "storageName": {
                        "value": "[variables('storageName')]"
                    },
                    "vnetName": {
                        "value": "[parameters('vnetName')]"
                    },
                    "publicIpName": {
                        "value": "[parameters('publicIpName')]"
                    },
                    "nicName": {
                        "value": "[parameters('nicName')]"
                    },
                    "vmName": {
                        "value": "[parameters('vmName')]"
                    },
                    "vmOsSku": {
                        "value": "[parameters('vmOsSku')]"
                    },
                    "dnsPrefix": {
                        "value": "[parameters('dnsPrefix')]"
                    },
                    "adminUsername": {
                        "value": "string"
                    },
                    "adminPassword": {
                        "value": "securestring"
                    }
                }
            }
        }
    ],
    "outputs": {
    }
}

{
    "status": "Failed",
    "error": {
        "code": "ResourceDeploymentFailure",
        "message": "The resource operation completed with terminal provisioning state 'Failed'.",
        "details": [
            {
                "code": "DeploymentOutputEvaluationFailed",
                "message": "Unable to evaluate template outputs: 'storageUriBase,storageUriVhdContainer'. Please see error details and deployment operations. Please see http://aka.ms/arm-debug for usage details.",
                "details": [
                    {
                        "code": "DeploymentOutputEvaluationFailed",
                        "target": "storageUriBase",
                        "message": "The template output 'storageUriBase' is not valid: Index (zero based) must be greater than or equal to zero and less than the size of the argument list.."
                    },
                    {
                        "code": "DeploymentOutputEvaluationFailed",
                        "target": "storageUriVhdContainer",
                        "message": "The template output 'storageUriVhdContainer' is not valid: Index (zero based) must be greater than or equal to zero and less than the size of the argument list.."
                    }
                ]
            }
        ]
    }
}
